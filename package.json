		"enzyme-to-json": "~3.3.5",
		"jest": "~23.6.0",
		"jest-coverage-badges": "~1.1.2",
		"prettier": "~1.16.4",
		"react-scripts": "~2.1.8",
		"ts-jest": "~24.0.0",
		"tslint": "~5.14.0",
		"tslint-config-prettier": "~1.18.0",
		"tslint-react": "~4.0.0",
		"typescript": "~3.4.1",
		"webpackbar": "~3.1.5"
	},
	"browserslist": [
		">0.2%",
		"not dead",
