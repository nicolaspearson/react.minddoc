{
	"name": "react.minddoc",
	"version": "0.0.1",
	"description": "A demo React app for MindDoc built using Typescript, MobX, SASS, and Webpack.",
	"repository": {
		"type": "git",
		"url": "https://github.com/nicolaspearson/react.minddoc.git"
	},
	"license": "MIT",
	"scripts": {
		"start": "craco start",
		"build": "craco build",
		"cra-start": "react-scripts start",
		"cra-build": "react-scripts build",
		"cra-test": "react-scripts test",
		"cra-eject": "react-scripts eject",
		"prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
		"tslint": "tslint --fix -c tslint.json -p tsconfig.json",
