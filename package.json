		"react-dom": "~16.8.6",
		"router5": "~7.0.2",
		"router5-plugin-browser": "~7.0.2",
		"styled-components": "~4.2.0",
		"typeface-lato": "~0.0.54"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "~7.4.0",
		"@craco/craco": "~3.6.0",
		"@types/classnames": "~2.2.7",
		"@types/enzyme": "~3.9.1",
		"@types/enzyme-adapter-react-16": "~1.0.5",
		"@types/jest": "~23.3.13",
		"@types/node": "~11.12.2",
		"@types/react": "~16.8.10",
